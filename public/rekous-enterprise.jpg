<script>
  var Webflow = Webflow || [];
  Webflow.push(function () {

    const transitionTexts = {
      "home": "empowering brands to grow with no-code",
      "work": "delivering excellence for ambitious brands",
      "services": "boosting growth at every stage",
      "culture": "cultivating a global hive-mind",
      "stories": "sharing our knowledge and skills"
    }

    window.addEventListener('pageshow', (event) => { 
      if (event.persisted) {
        document.querySelector('.c-page_transition').style.display = 'none'
      }
    })

    // add clicked page's name into transition -------
    const pageTitle = document.querySelector('#page-title')
    const pageText = document.querySelector('#page-text')
    const siteURL = "http://" + top.location.host.toString()
    const allLinks = document.querySelectorAll("a[href^='"+siteURL+"'], a[href^='/'], a[href^='./'], a[href^='../']")
    // function that will return the slug of the clicked link
    let slug = url => new URL(url).pathname.split('/')[1];

    allLinks.forEach(link => {
      link.addEventListener('click', () => {
        let path = slug(link.href)

        // replace dash with spaces and replace the page title if there is a path
        if (path) { 
          pageTitle.innerHTML = path.replace(/-/g, ' ')
          pageText.innerHTML = transitionTexts[path.replace(/-/g, ' ')] || transitionTexts["home"]
        }
        // if null then write "home"
        else { 
          pageTitle.innerHTML = 'home' 
          pageText.innerHTML = transitionTexts["home"]
        }
      })
    })

    // nav bg change ----------
    if ($('[nav-change]').length) { //transition existence if
      let original
      const nav = $(".c-nav")
      if ($('[nav-change="dark"]').length) {//dark nav if
        gsap.to(".c-nav", {
          scrollTrigger : {
            trigger: "[nav-change='dark']",
            start: "top top",
            toggleActions: "play none none reverse",
            onEnter() {
              if (nav.hasClass('cc-white')) {
                original = 'cc-white';
                nav.removeClass('cc-white')
              }
              nav.addClass('cc-dark');
            },
            onEnterBack() {
              nav.addClass(original);
              nav.removeClass('cc-dark');
            }
          },
        })
      } //end dark nav if
      if ($('[nav-change="light"]').length) {//light nav if
        gsap.to(".c-nav", {
          scrollTrigger : {
            trigger: "[nav-change='light']",
            start: "top top",
            toggleActions: "play none none reverse",
            onEnter() {
              if (nav.hasClass('cc-dark')) {
                original = 'cc-dark';
                nav.removeClass('cc-dark');
              }
              nav.addClass('cc-white');
            },
            onEnterBack() {
              nav.addClass(original);
              nav.removeClass('cc-white');
            }
          },
        })
      }// end light nav if
    }//end existence if
    // end of nav bg change ----
  })
